use terminal

Color := terminal.Color
Style := terminal.Style
fg-color := terminal.fg-color
style := terminal.style

colors := []
color-keys := dict-keys(Color)
for i in range(0, len(color-keys) - 1) do
  list-add(colors, ref(Color, nth(color-keys, i)))
end

for i in range(0, len(colors) - 1) do
  write(fg-color(nth(colors, i)), "Hello\n")
end
