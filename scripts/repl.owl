history := list()

archimedes := " ~___~  Owl (0.0.0-dev)
 {O,o}  run with 'help' for list of commands
/)___)  enter '?' to show help for repl
  ' '"

fun repl()
  line := read-line("> ")
  done := #f

  cond
    line eq ":q" do
      done = #t
    end
    line eq "?" do
      echo("Help is work in progress.")
    end
  else
    echo(eval(line))
  end

  if done then
    echo("bye.")
  else
    repl()
  end
end

echo(archimedes)
repl()
