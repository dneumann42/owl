history := list()

archimedes := " ~___~  Owl (0.0.0-dev)
 {O,o}  run with 'help' for list of commands
/)___)  enter '?' to show help for repl
  ' '"

fun repl()
  line := read-line("> ")
  if line eq ":q" then
    echo("bye.")
  else
    echo(eval(line))
    repl()
  end
end

echo(archimedes)
repl()
