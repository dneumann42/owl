;; core library functions needed for most scripts

fun range(min, max)
  fn() if min > max then
    false
  else
    idx := min
    min = min + 1
    idx
  end
end

fun items(list)
  i := 0
  fn() if i < len(list) then
    itm = nth(item, i)
    i = i + 1
    itm
  else
    false
  end
end

{
  range: range
}
